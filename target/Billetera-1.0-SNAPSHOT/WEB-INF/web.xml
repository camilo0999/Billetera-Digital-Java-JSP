<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>ControladorEstudianteRegistro</servlet-name>
        <servlet-class>com.mycompany.billetera.Controlador.ControladorEstudianteRegistro</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AdminServicios</servlet-name>
        <servlet-class>com.mycompany.billetera.Servicios.AdminServicios</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ListarEstudiantesAdmin</servlet-name>
        <servlet-class>com.mycompany.billetera.Controlador.ListarEstudiantesAdmin</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ControladorEstudianteRegistro</servlet-name>
        <url-pattern>/ControladorEstudianteRegistro</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AdminServicios</servlet-name>
        <url-pattern>/AdminServicios</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ListarEstudiantesAdmin</servlet-name>
        <url-pattern>/ListarEstudiantesAdmin</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
